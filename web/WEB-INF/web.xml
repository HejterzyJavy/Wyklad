<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <welcome-file-list>
   <welcome-file>Log.jsp</welcome-file>
</welcome-file-list>
    
    <servlet>
        <servlet-name>UserKontroler</servlet-name>
        <servlet-class>kontroler.UserKontroler</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CarKontroler</servlet-name>
        <servlet-class>kontroler.CarKontroler</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RentKontroler</servlet-name>
        <servlet-class>kontroler.RentKontroler</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ZdjecieZBazy</servlet-name>
        <servlet-class>util.ZdjecieZBazy</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserKontroler</servlet-name>
        <url-pattern>/UserKontroler</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CarKontroler</servlet-name>
        <url-pattern>/CarKontroler</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RentKontroler</servlet-name>
        <url-pattern>/RentKontroler</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ZdjecieZBazy</servlet-name>
        <url-pattern>/ZdjecieZBazy</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
